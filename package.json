{
  "name": "demo7",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:local:dev": "run-p start:local:api start:local:client:dev",
    "start:local:api": "wait-on -d 500 node_modules/meteor-client.js && cd api && meteor run",
    "start:local:client:dev": "wait-on node_modules/meteor-client.js && ng serve",
    "prestart:local:client:dev": "path-exists node_modules/meteor-client.js || npm run build:meteor-client",
    "build:meteor-client": "meteor-client bundle -s api",
    "reset:api": "cd api && meteor reset",
    "reset:api:db": "rimraf api/.meteor/local/db",
    "clean": "npm run clean:dist",
    "clean:build": "run-p clean:meteor-client clean:dist",
    "clean:meteor-client": "rimraf node_modules/meteor-client.js",
    "clean:dist": "rimraf dist/demo7"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.3",
    "@angular/common": "^6.1.3",
    "@angular/compiler": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/forms": "^6.1.3",
    "@angular/http": "^6.1.3",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "@angular/router": "^6.1.3",
    "@babel/runtime": "7.0.0-beta.55",
    "core-js": "^2.5.7",
    "meteor-node-stubs": "^0.4.1",
    "meteor-rxjs": "^0.4.14",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.4",
    "@angular/cli": "~6.1.4",
    "@angular/compiler-cli": "^6.1.3",
    "@angular/language-service": "^6.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/meteor": "^1.4.16",
    "@types/node": "~10.7.1",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.0",
    "meteor-client-bundler": "^0.5.9",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "path-exists-cli": "^1.0.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "wait-on": "^2.1.0"
  }
}
